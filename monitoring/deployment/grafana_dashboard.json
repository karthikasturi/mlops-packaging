{
  "dashboard": {
    "title": "ML Model Monitoring Dashboard",
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate (req/s)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(model_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} {{status}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "Prediction Count",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "model_predictions_total"
          }
        ]
      },
      {
        "id": 3,
        "title": "Active Requests",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "model_server_active_requests"
          }
        ]
      },
      {
        "id": 4,
        "title": "Request Latency (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(model_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(model_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(model_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99"
          }
        ]
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(model_errors_total[5m])",
            "legendFormat": "{{error_type}}"
          }
        ]
      },
      {
        "id": 6,
        "title": "CPU Usage (%)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "model_server_cpu_usage_percent"
          }
        ]
      },
      {
        "id": 7,
        "title": "Memory Usage (MB)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "model_server_memory_usage_mb"
          }
        ]
      },
      {
        "id": 8,
        "title": "Request Status Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum by (status) (model_requests_total)"
          }
        ]
      },
      {
        "id": 9,
        "title": "Throughput (predictions/min)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 24},
        "targets": [
          {
            "expr": "rate(model_predictions_total[1m]) * 60"
          }
        ]
      },
      {
        "id": 10,
        "title": "Average Latency (ms)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 24},
        "targets": [
          {
            "expr": "rate(model_request_duration_seconds_sum[5m]) / rate(model_request_duration_seconds_count[5m]) * 1000"
          }
        ]
      }
    ]
  }
}
